<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <!-- Конфигурация для веб-сервера -->
    <dictionary>
        <entry>
            <name>server.hostname</name>
            <value>example.com</value>
        </entry>
        <entry>
            <name>server.port</name>
            <value>8080</value>
        </entry>
    </dictionary>

    <!-- Константы для веб-сервера -->
    <constant>
        <name>MAX_REQUESTS</name>
        <value>500</value>
    </constant>
    <constant>
        <name>TIMEOUT</name>
        <value>60</value>
    </constant>
</configuration>
